module acme {

    const string pose_topic_name = "Pose Topic Name";
    const string qos_library = "MyLibrary";
    const string qos_profile = "MyProfile";
    
    @nested
    struct Point {
        double x;
        double y;
        double z;
    };

    @nested
    struct Quaternion {
        double x;
        double y;
        double z;
        double w;
    };

    struct Pose {
        @key long obj_id;
        Point position;
        Quaternion orientation;
    };
};